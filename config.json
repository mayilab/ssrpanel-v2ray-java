    "protocol": "freedom",
     "settings": {
      }},
  {
      "protocol": "blackhole",
      "settings": {},
      "tag": "block"
    }
  ],
 "routing": {
    "domainStrategy": "AsIs",
    "rules": [{
      "type": "field",
      "inboundTag": [ "api" ],
      "outboundTag": "api"
},
    {
        "type": "field",
        "outboundTag": "block",
        "protocol": [
